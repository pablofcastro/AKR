                     & (c=0)
                     & (v1=0)
                     & (v2=0)
                     & (v3=0)
                     & (v4=0);

    formula complete = (c>=2);

    module plts
        s  : [0..4];
        c  : [0..4];
        v1 : [0..1];
        v2 : [0..1];
        v3 : [0..1];
        v4 : [0..1];

        [one] (s=0) -> 0.80:(s'=1)&(c'=min(4,c+(1-v1)))&(v1'=1) + 0.20:(s'=1)&(c'=c)&(v1'=1);
        [two] (s=0) -> 0.46:(s'=2)&(c'=min(4,c+(1-v2)))&(v2'=1) + 0.54:(s'=2)&(c'=c)&(v2'=1);
        [two] (s=1) -> 0.46:(s'=2)&(c'=min(4,c+(1-v2)))&(v2'=1) + 0.54:(s'=2)&(c'=c)&(v2'=1);
        [three] (s=1) -> 0.85:(s'=3)&(c'=min(4,c+(1-v3)))&(v3'=1) + 0.15:(s'=3)&(c'=c)&(v3'=1);
        [one] (s=2) -> 0.80:(s'=1)&(c'=min(4,c+(1-v1)))&(v1'=1) + 0.20:(s'=1)&(c'=c)&(v1'=1);
        [four] (s=2) -> 0.21:(s'=4)&(c'=min(4,c+(1-v4)))&(v4'=1) + 0.79:(s'=4)&(c'=c)&(v4'=1);
        [one] (s=3) -> 0.80:(s'=1)&(c'=min(4,c+(1-v1)))&(v1'=1) + 0.20:(s'=1)&(c'=c)&(v1'=1);
        [four] (s=3) -> 0.21:(s'=4)&(c'=min(4,c+(1-v4)))&(v4'=1) + 0.79:(s'=4)&(c'=c)&(v4'=1);
        [two] (s=4) -> 0.46:(s'=2)&(c'=min(4,c+(1-v2)))&(v2'=1) + 0.54:(s'=2)&(c'=c)&(v2'=1);
        [three] (s=4) -> 0.85:(s'=3)&(c'=min(4,c+(1-v3)))&(v3'=1) + 0.15:(s'=3)&(c'=c)&(v3'=1);
    endmodule
endplts

perception :
    (one.two.four.three)+(two.one.three.four)
endperception

property : Kh(launch,complete) >= 0.25
endproperty
